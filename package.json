  "author": "sparklewhy@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "coveralls": "^2.11.16",
    "expect.js": "^0.3.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "uglify-js": "^2.8.3"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "chokidar": "^1.6.1",
    "connect": "^3.6.0",
    "http-proxy": "^1.16.2",
    "lodash": "^4.17.4",
    "mime": "^1.3.4",
    "opn": "^4.0.2",
    "resolve": "^1.3.2",
